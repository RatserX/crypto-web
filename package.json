{
  "name": "crypto-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run src --reporter=verbose",
    "test:watch": "vitest src --reporter=verbose",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "cleanup": "npm run lint && npm run format",
    "format": "prettier --write --plugin prettier-plugin-svelte ./**/*.{cjs,js,svelte}",
    "lint": "eslint ./**/*.{cjs,js,svelte} --fix"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^3.2.2",
    "@types/crypto-js": "^4.1.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-svelte": "^2.34.0",
    "gh-pages": "^6.0.0",
    "jsdom": "^22.0.0",
    "postcss": "^8.4.31",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.2",
    "svelte-heros-v2": "^0.4.2",
    "svelte-preprocess": "^5.0.3",
    "svelte-simples": "^0.6.1",
    "tailwindcss": "^3.3.2",
    "vite": "^4.3.4",
    "vitest": "^0.31.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "classnames": "^2.3.2",
    "crypto-js": "^4.2.0",
    "flowbite": "^1.6.5",
    "flowbite-svelte": "^0.34.10",
    "svelte-select": "^5.6.0"
  },
  "homepage": "https://ratser.github.io/crypto-web"
}
